<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>de.bb.tools.bnm</groupId>
		<artifactId>bnm</artifactId>
		<version>0.1.3</version>
	</parent>
	<groupId>de.bb.tools.bnm</groupId>
	<artifactId>eclipse</artifactId>
	<version>0.3.0-SNAPSHOT</version>
	<packaging>jar</packaging>
	<dependencies>
		<dependency>
			<groupId>de.bb.tools.bnm</groupId>
			<artifactId>core</artifactId>
			<version>0.3.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>org.eclipse.platform</groupId>
			<artifactId>org.eclipse.jface</artifactId>
			<version>3.19.0.v20200218-1607</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.eclipse.platform</groupId>
			<artifactId>org.eclipse.swt.win32.win32.x86_64</artifactId>
			<version>3.114.0.v20200304-0601</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.eclipse.platform</groupId>
			<artifactId>org.eclipse.ui.console</artifactId>
			<version>3.9.100.v20200215-1209</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.eclipse.platform</groupId>
			<artifactId>org.eclipse.jdt.core</artifactId>
			<version>3.21.0.v20200304-1404</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.eclipse.platform</groupId>
			<artifactId>org.eclipse.jdt.launching</artifactId>
			<version>3.17.0.v20200215-0651</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.eclipse.platform</groupId>
			<artifactId>org.eclipse.ltk.core.refactoring</artifactId>
			<version>3.10.400.v20200215-1156</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.eclipse.platform</groupId>
			<artifactId>org.eclipse.ltk.ui.refactoring</artifactId>
			<version>3.10.200.v20200221-2020</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.eclipse.platform</groupId>
			<artifactId>org.eclipse.ui.ide</artifactId>
			<version>3.17.0.v20200217-1511</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.eclipse.platform</groupId>
			<artifactId>org.eclipse.pde.core</artifactId>
			<version>3.15.200.v20220527-1237</version>
			<scope>provided</scope>
		</dependency>
	</dependencies>
	<build>
		<resources>
			<resource>
				<directory>.</directory>
				<includes>
					<include>icons/*</include>
					<include>plugin.xml</include>
					<include>META-INF/MANIFEST.MF</include>
					<include>COPYING</include>
				</includes>
			</resource>
			<resource>
				<directory>src/main/java</directory>
				<targetPath>src</targetPath>
			</resource>
		</resources>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-eclipse-plugin</artifactId>
					<configuration>
						<pde>true</pde>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
</project>
